<?xml version="1.0" encoding="utf-8"?>
<configuration>
 
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  
  <system.serviceModel>
    <bindings>
      <wsHttpBinding>
        <binding name="WSHttpBinding_IReservationManagment">
          <security mode="TransportWithMessageCredential">
            <transport clientCredentialType="None" />
            <message clientCredentialType="UserName" />
          </security>
        </binding>
        <binding name="WSHttpBinding_IReservationManagmentUnsecure" />
        <binding name="WSHttpBinding_IUserManagment">
          <security mode="TransportWithMessageCredential">
            <transport clientCredentialType="None" />
            <message clientCredentialType="UserName" />
          </security>
        </binding>
        <binding name="WSHttpBinding_IUserManagmentUnsecure" />
      </wsHttpBinding>
    </bindings>
    <client>
      <endpoint address="https://localhost:44300/ReservationManagment.svc"
        binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IReservationManagment"
        contract="ReservationService.IReservationManagment" name="WSHttpBinding_IReservationManagment" />
      <endpoint address="http://localhost:2106/ReservationManagment.svc/unsec"
        binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IReservationManagmentUnsecure"
        contract="ReservationService.IReservationManagmentUnsecure"
        name="WSHttpBinding_IReservationManagmentUnsecure">

      </endpoint>
      <endpoint address="https://localhost:44300/UserManagment.svc"
        binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IUserManagment"
        contract="UserService.IUserManagment" name="WSHttpBinding_IUserManagment" />
      <endpoint address="http://localhost:2106/UserManagment.svc/unsec"
        binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IUserManagmentUnsecure"
        contract="UserService.IUserManagmentUnsecure" name="WSHttpBinding_IUserManagmentUnsecure">
   
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>